//Estrutura de repetição com teste lógico no fim

var c = 1
if (c == 1) {
do {
    console.log(`1 X ${c} = ${c}`)
} while (c<=10)
}
else if (c == 2) {
do {
    console.log(`2 X ${c} = ${c}`)
    c = c+2
} while(c<=20) }

else if(c==3) {
do {
    console.log(`3 X ${c} = ${c}`)
    c = c+3
} while(c<=30) }